{
  "name": "global-prefix",
  "description": "Get the npm global path prefix.",
  "version": "1.0.2",
  "homepage": "https://github.com/jonschlinkert/global-prefix",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/global-prefix",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "expand-tilde": "^2.0.2",
    "homedir-polyfill": "^1.0.1",
    "ini": "^1.3.4",
    "is-windows": "^1.0.1",
    "which": "^1.2.14"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.2"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "global-modules",
        "global-paths"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}